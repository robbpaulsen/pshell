# yaml-language-server: $schema=https://aka.ms/configuration-dsc-schema/0.2
# Reference: https://github.com/microsoft/winget-create#building-the-client
# WinGet Configure file Generated By Dev Home.

properties:
  resources:
  - resource: Microsoft.WinGet.DSC/WinGetPackage
    directives:
      description: Installing Git
      allowPrerelease: true
      securityContext: current
    settings:
      id: "Git.Git"
      source: winget
    id: Git.Git
  - resource: GitDsc/GitClone
    directives:
      description: 'Cloning: anything-llm'
      allowPrerelease: true
      securityContext: current
    settings:
      httpsUrl: https://github.com/Mintplex-Labs/anything-llm.git
      rootDirectory: O:\developer\Documents\repos\anything-llm
    id: 'Clone anything-llm: O:\developer\Documents\repos\anything-llm'
    dependsOn:
    - Git.Git
  configurationVersion: 0.2.0
